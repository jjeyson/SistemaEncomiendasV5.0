<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Creative - Bootstrap 3 Responsive Admin Template">
    <meta name="author" content="GeeksLabs">
    <meta name="keyword" content="Creative, Inicio, Admin, Template, Theme, Bootstrap, Responsive, Retina, Minimal">
    <link rel="shortcut icon" href="img/favicon.png">
    <title>Principal</title>
    <link href="../Bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="../Bootstrap/css/bootstrap-theme.css" rel="stylesheet">
    <link href="../Bootstrap/css/elegant-icons-style.css" rel="stylesheet" />
    <link href="../Bootstrap/css/font-awesome.min.css" rel="stylesheet" />
    <link href="../Bootstrap/assets/fullcalendar/fullcalendar/bootstrap-fullcalendar.css" rel="stylesheet" />
    <link href="../Bootstrap/assets/fullcalendar/fullcalendar/fullcalendar.css" rel="stylesheet" />
    <link href="../Bootstrap/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.css" rel="stylesheet" type="text/css" media="screen" />
    <link rel="../Bootstrap/stylesheet" href="css/owl.carousel.css" type="text/css">
    <link href="../Bootstrap/css/jquery-jvectormap-1.2.2.css" rel="stylesheet">
    <link rel="../Bootstrap/stylesheet" href="css/fullcalendar.css">
    <link href="../Bootstrap/css/widgets.css" rel="stylesheet">
    <link href="../Bootstrap/css/style.css" rel="stylesheet">
    <link href="../Bootstrap/css/style-responsive.css" rel="stylesheet" />
    <link href="../Bootstrap/css/xcharts.min.css" rel=" stylesheet">
    <link href="../Bootstrap/css/jquery-ui-1.10.4.min.css" rel="stylesheet">
    <link href="~/Bootstrap/js/jquery-ui-1.11.2/jquery-ui.css" rel="stylesheet" />
    <link href="~/Bootstrap/js/jquery-ui-1.11.2/jquery-ui.min.css" rel="stylesheet" />
</head>
<body>
    <section id="container" class="">

        <header class="header dark-bg">
            <div class="toggle-nav">
                <div class="icon-reorder tooltips" data-original-title="Toggle Navigation" data-placement="bottom"></div>
            </div>
            <a href="@Url.Action("Home","IniciarSesion")" class="logo">BIENV<span class="lite">ENIDO</span></a>
            <div class="top-nav notification-row">
                <ul class="nav pull-right top-menu">
                    <li class="dropdown">
                        <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                            @{

                                C3_Dominio.Entidades.Usuario usuario = (C3_Dominio.Entidades.Usuario)Session["usuario"];
                                C3_Dominio.Entidades.TipoUsuario tu = (C3_Dominio.Entidades.TipoUsuario)Session["tipoUsuario"];
                                if (usuario != null)
                                {
                                    <span class="username"><i class="icon_profile"></i> @usuario.NombreUsuario @usuario.ApellidosUsuario @usuario.sucursal.IdSucursal</span>
                                    <b class="caret"></b>
                                }
                            }
                        </a>
                        <ul class="dropdown-menu extended logout">
                            <li class="eborder-top">
                                <a href="#"><i class="icon_profile"></i> Mi perfil</a>
                            </li>
                            <li>
                                <a href="@Url.Action("CerrarSesion","InicioSesion")"><i class="icon_key_alt"></i> Cerrar Sesion</a>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
        </header>
        @{
            if (usuario != null && tu.nombre == "Administrador")
            {
                <aside>
                    <div id="sidebar" class="nav-collapse ">
                        <ul class="sidebar-menu">
                            <li class="active">
                                <a class="" href="@Url.Action("Home", "InicioSesion")">
                                    <i class="icon_house_alt"></i>
                                    <span>Inicio</span>
                                </a>
                            </li>
                            <li>
                                <a href="@Url.Action("RegistroCliente","Cliente")" class="">
                                    <i class="icon_profile"></i>
                                    <span>Gestionar Clientes</span>
                                </a>
                            </li>
                            <li>
                                <a href="@Url.Action("ListarReporte", "ReporteEncomiendas")" class="">
                                    <i class="icon_calendar"></i>
                                    <span>Reportes</span>
                                </a>
                            </li>

                            <li>
                                <a href="@Url.Action("ListaUsuario", "Usuario")" class="">
                                    <i class="icon_profile"></i>
                                    <span>Gestionar Usuarios</span>
                                </a>
                            </li>

                            <li class="sub-menu">
                                <a href="javascript:;" class="">
                                    <i class="icon_document_alt"></i>
                                    <span>Gestionar Procesos</span>
                                    <span class="menu-arrow arrow_carrot-right"></span>
                                </a>
                                <ul class="sub">
                                    <li>
                                        <form id="frm" method="post" action="~/EnvioEncomienda/GestionarEnvioEncomienda">
                                            <input type="submit" value="Gestionar Envio" class="btn btn-primary btn-lg btn-block" />
                                            <input name="txtIdSucursal" type="text" id="txtIdSucursal" style='visibility:hidden' value="@usuario.sucursal.IdSucursal" />
                                        </form>
                                    </li>
                                    <li>
                                        <form id="frm" method="post" action="~/RecepcionEncomienda/GestionarRecepcionEncomienda">
                                            <input type="submit" value="Gestionar Recepcion" class="btn btn-primary btn-lg btn-block" />
                                            <input name="txtIdSucursal" type="text" id="txtIdSucursal" style='visibility:hidden' value="@usuario.sucursal.IdSucursal" />
                                        </form>
                                    </li>
                                </ul>
                            </li>
                            <li>
                                <a href="@Url.Action("ListarEnvios", "EnvioEncomienda")" class="">
                                    <i class="icon_document_alt"></i>
                                    <span>Gestionar Encomienda</span>
                                </a>
                            </li>
                        </ul>
                    </div>
                </aside>
            }
            else if (usuario != null && tu.nombre == "Recepcionista")
            {
                <aside>
                    <div id="sidebar" class="nav-collapse ">
                        <ul class="sidebar-menu">
                            <li class="active">
                                <a class="" href="@Url.Action("Home", "InicioSesion")">
                                    <i class="icon_house_alt"></i>
                                    <span>Inicio</span>
                                </a>
                            </li>
                            <li>
                                <a href="@Url.Action("RegistroCliente","Cliente")" class="">
                                    <i class="icon_desktop"></i>
                                    <span>Gestionar Cliente</span>
                                </a>
                            </li>

                            <li class="sub-menu">
                                <a href="javascript:;" class="">
                                    <i class="icon_document_alt"></i>
                                    <span>Gestionar Procesos</span>
                                    <span class="menu-arrow arrow_carrot-right"></span>
                                </a>
                                <ul class="sub">
                                    <li>
                                        <form id="frm" method="post" action="~/EnvioEncomienda/GestionarEnvioEncomienda">
                                            <input type="submit" value="Gestionar Envío" class="btn btn-primary btn-lg btn-block" />
                                            <input name="txtIdSucursal" type="text" id="txtIdSucursal" style='visibility:hidden' value="@usuario.sucursal.IdSucursal" />
                                        </form>
                                    </li>
                                    <li>
                                        <form id="frm" method="post" action="~/RecepcionEncomienda/GestionarRecepcionEncomienda">
                                            <input type="submit" value="Gestionar Recepcion" class="btn btn-primary btn-lg btn-block" />
                                            <input name="txtIdSucursal" type="text" id="txtIdSucursal" style='visibility:hidden' value="@usuario.sucursal.IdSucursal" />
                                        </form>
                                    </li>
                                </ul>
                            </li>
                            <li>
                                <a href="@Url.Action("ListarEnvios", "EnvioEncomienda")" class="">
                                    <i class="icon_document_alt"></i>
                                    <span>Gestionar Encomienda</span>
                                </a>
                            </li>
                        </ul>
                    </div>
                </aside>
            }
            else if (usuario != null && tu.nombre == "Encargado del Area de Despacho")
            {
                <aside>
                    <div id="sidebar" class="nav-collapse ">
                        <ul class="sidebar-menu">
                            <li class="active">
                                <a class="" href="@Url.Action("Home", "InicioSesion")">
                                    <i class="icon_house_alt"></i>
                                    <span>Inicio</span>
                                </a>
                            </li>
                            <li>
                                <a href="@Url.Action("GestionarEnvioEncomienda ", "EnvioEncomienda")" class="">
                                    <i class="icon_desktop"></i>
                                    <span>Gestionar Envio</span>
                                </a>
                            </li>
                        </ul>
                    </div>
                </aside>
            }
        }
        <section id="main-content">
            <section class="wrapper">
                <div class="row">
                    <div class="col-lg-12">
                        <h3 class="page-header"><i class="fa fa-laptop"></i> Inicio</h3>
                        <ol class="breadcrumb">
                            <li><i class="fa fa-laptop"></i>Inicio</li>
                        </ol>
                    </div>
                </div>
                @RenderBody()
            </section>
        </section>
    </section>
    <script src="~/Bootstrap/js/jquery-ui-1.11.2/external/jquery/jquery.js"></script>
    <script src="~/Bootstrap/js/jquery-ui-1.11.2/jquery-ui.js"></script>
    @*<script src="~/Bootstrap/js/jquery-ui-1.11.2/jquery-ui.min.js"></script>*@
    <!-- bootstrap -->
    <script src="../Bootstrap/js/bootstrap.min.js"></script>
    <!-- nice scroll -->
    <script src="../Bootstrap/js/jquery.scrollTo.min.js"></script>
    <script src="../Bootstrap/js/jquery.nicescroll.js" type="text/javascript"></script>
    <script src="../Bootstrap/assets/jquery-knob/js/jquery.knob.js"></script>
    <script src="../Bootstrap/js/jquery.sparkline.js" type="text/javascript"></script>
    <script src="../Bootstrap/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.js"></script>
    <script src="../Bootstrap/js/owl.carousel.js"></script>
    <!-- jQuery full calendar -->
    <
    <script src="../Bootstrap/js/fullcalendar.min.js"></script> <!-- Full Google Calendar - Calendar -->
    <script src="../Bootstrap/assets/fullcalendar/fullcalendar/fullcalendar.js"></script>
    <script src="../Bootstrap/js/calendar-custom.js"></script>
    <script src="../Bootstrap/js/jquery.rateit.min.js"></script>
    <script src="../Bootstrap/js/jquery.customSelect.min.js"></script>
    <script src="../Bootstrap/assets/chart-master/Chart.js"></script>
    <script src="../Bootstrap/js/scripts.js"></script>
    <script src="../Bootstrap/js/sparkline-chart.js"></script>
    <script src="../Bootstrap/js/easy-pie-chart.js"></script>
    <script src="../Bootstrap/js/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="../Bootstrap/js/jquery-jvectormap-world-mill-en.js"></script>
    <script src="../Bootstrap/js/xcharts.min.js"></script>
    <script src="../Bootstrap/js/jquery.autosize.min.js"></script>
    <script src="../Bootstrap/js/jquery.placeholder.min.js"></script>
    <script src="../Bootstrap/js/gdp-data.js"></script>
    <script src="../Bootstrap/js/morris.min.js"></script>
    <script src="../Bootstrap/js/sparklines.js"></script>
    <script src="../Bootstrap/js/charts.js"></script>
    <script src="../Bootstrap/js/jquery.slimscroll.min.js"></script>


    <script>



        //knob
        $(function () {
            $(".knob").knob({
                'draw': function () {
                    $(this.i).val(this.cv + '%')
                }
            })
        });

        //carousel
        $(document).ready(function () {
            $("#owl-slider").owlCarousel({
                navigation: true,
                slideSpeed: 300,
                paginationSpeed: 400,
                singleItem: true

            });
        });

        //custom select box

        $(function () {
            $('select.styled').customSelect();
        });

        /* ---------- Map ---------- */
        $(function () {
            $('#map').vectorMap({
                map: 'world_mill_en',
                series: {
                    regions: [{
                        values: gdpData,
                        scale: ['#000', '#000'],
                        normalizeFunction: 'polynomial'
                    }]
                },
                backgroundColor: '#eef3f7',
                onLabelShow: function (e, el, code) {
                    el.html(el.html() + ' (GDP - ' + gdpData[code] + ')');
                }
            });
        });



    </script>
</body>
</html>

